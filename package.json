{
    "name": "trackball",
    "title": "Trackball.js",
    "version": "1.3.1",
    "description": "The RAW library to add a virtual Trackball to your DOM",
    "homepage": "https://raw.org/article/trackball-rotation-using-quaternions/",
    "bugs": "https://github.com/rawify/Trackball.js/issues",
    "keywords": [
        "arcball",
        "trackball",
        "rotation",
        "orbit",
        "sphere",
        "html5",
        "css3",
        "3d",
        "quaternion"
    ],
    "private": false,
    "main": "./dist/trackball.js",
    "module": "./dist/trackball.mjs",
    "browser": "./dist/trackball.min.js",
    "unpkg": "./dist/trackball.min.js",
    "types": "./trackball.d.ts",
    "exports": {
        ".": {
            "import": {
                "types": "./trackball.d.mts",
                "default": "./dist/trackball.mjs"
            },
            "require": {
                "types": "./trackball.d.ts",
                "default": "./dist/trackball.js"
            },
            "browser": {
                "types": "./trackball.d.ts",
                "default": "./dist/trackball.min.js"
            },
            "default": {
                "types": "./trackball.d.ts",
                "default": "./dist/trackball.js"
            }
        },
        "./package.json": "./package.json"
    },
    "typesVersions": {
        "<4.7": {
            "*": [
                "trackball.d.ts"
            ]
        }
    },
    "sideEffects": false,
    "repository": {
        "type": "git",
        "url": "git@github.com:rawify/Trackball.js.git"
    },
    "funding": {
        "type": "github",
        "url": "https://github.com/sponsors/rawify"
    },
    "author": {
        "name": "Robert Eisele",
        "email": "robert@raw.org",
        "url": "https://raw.org/"
    },
    "license": "MIT",
    "engines": {
        "node": "*"
    },
    "directories": {
        "example": "examples"
    },
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "build": "crude-build Trackball"
    },
    "dependencies": {
        "quaternion": "2.1.0"
    },
    "devDependencies": {
        "crude-build": "^0.1.2"
    }
}